class Product (object):
    height = 0
    width = 0
    depth = 0
    weight = 0
    tpnb = ""
    
    def __init__(self,height,width, depth, weight, tpnb):
        self.height = height
        self.width = width
        self.depth = depth
        self.weight = weight
        self.tpnb = tpnb

def makeProduct(height, width, depth, weight, tpnb): 
    product = Product(height, width, depth, weight, tpnb)
    return product